@using KütüphaneOtomasyonu.Models.Entity
@model List<Tblİşlemler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<br />

<table class="table table-bordered">
    <tr style="background-color:black;color:white;">
        <th>KİTAP</th>
        <th>ÜYE</th>
        <th>PERSONEL</th>
        <th>ALIŞ TARİHİ</th>
        <th>AZAMİ TESLİM</th>
        <th>İADE TARİHİ</th>

     
    </tr>

    @foreach (var x in Model)
    {
        <tr>

            <td>@x.TblKitaplar.Ad</td>
            <td>@x.TblÜyeler.Ad @x.TblÜyeler.Soyad</td>
            <td>@x.TblPersonel.AdSoyad</td>
            <td>@Convert.ToDateTime(x.AlışTarihi).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(x.TeslimTarihi).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(x.İadeTarihi).ToString("dd/MM/yyyy")</td>

        </tr>
    }
</table>



